{
  "name": "File-manager",
  "version": "0.1.0",
  "description": "Simple file manager for some test purposes",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "gulp": "^3.8.7",
    "babelify": "~6.0.1",
    "gulp-connect": "~2.2.0",
    "gulp-sass": "~1.3.3",
    "require-dir": "~0.3.0",
    "vinyl-source-stream": "~1.1.0",
    "vinyl-buffer": "~1.0.0",
    "gulp-util": "~3.0.4",
    "reactify": "~1.1.0",
    "watchify": "~3.1.0",
    "browserify": "~9.0.7",
    "gulp-sass-bulk-import": "~0.2.0",
    "api-mock": "~0.2.2",
    "jest-cli": "~0.4.0",
    "babel-jest": "~5.0.1"
  },
  "scripts": {
    "dev": "gulp --type dev",
    "start": "NODE_PATH=src/js gulp --type dev",
    "test": "NODE_PATH=src/js jest src/js",
    "build": "gulp build",
    "mock-api": "node node_modules/api-mock/bin/api-mock api-mock.md --port 3030"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "dependencies": {
    "es6-promise": "^2.1.1",
    "flux": "~2.0.1",
    "lodash": "~3.6.0",
    "material-ui": "~0.7.3",
    "material-ui-sass": "~0.7.2",
    "object-assign": "~2.0.0",
    "react": "~0.13.1",
    "react-tap-event-plugin": "~0.1.6",
    "xhr": "~2.0.1",
    "moment": "~2.10.2"
  }
}
